{
  "name": "planta",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm pre:build && next build",
    "build:analyze": "ANALYZE=true pnpm build",
    "build-storybook": "storybook build",
    "copy-pdf-worker": "cp node_modules/.pnpm/pdfjs-dist@2.16.105/node_modules/pdfjs-dist/build/pdf.worker.min.js public/pdf.worker.js",
    "dev": "pnpm pre:dev && next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "format:check": "prettier . --check",
    "format:fix": "prettier --write --list-different .",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx .",
    "pre:build": "pnpm pre",
    "pre": "pnpm copy-pdf-worker",
    "pre:dev": "pnpm pre",
    "prepare": "husky install",
    "start": "next start",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:update": "vitest -u",
    "test:watch": "vitest --watch",
    "validate-types": "tsc --project ./tsconfig.json"
  },
